import jakarta.persistence*

@Entity
public class Genre {

    @Id
    @GeneratedValue
    private Long id;
    private String name;

    // Getters È™i Setters
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}